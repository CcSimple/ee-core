{
  "name": "ee-bin-ts",
  "version": "0.1.0-beta.2",
  "description": "ee bin for typescript",
  "main": "index.js",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": {
        "types": "./typings/index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./typings/esm.d.mts",
        "default": "./index.mjs"
      },
      "default": "./index.js"
    },
    "./index.mjs": {
      "types": "./typings/esm.d.mts",
      "import": "./index.mjs"
    }
  },
  "scripts": {
    "check": "npm run check:type",
    "check:type": "npm run check:type:js && npm run check:type:ts",
    "check:type:ts": "tsd && tsc -p tsconfig.ts.json",
    "check:type:js": "tsc -p tsconfig.js.json"
  },
  "author": "",
  "license": "ISC",
  "bin": {
    "ee-bin-tsm": "./index.mjs",
    "ee-bin-tsc": "index.js"
  },
  "dependencies": {
    "@types/node": "^20.9.0",
    "adm-zip": "^0.4.11",
    "bytenode": "^1.3.6",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "config-file-ts": "^0.2.8-rc1",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^10.0.0",
    "globby": "^10.0.0",
    "is-type-of": "^1.2.1",
    "javascript-obfuscator": "^4.0.2",
    "json5": "^2.2.3",
    "mkdirp": "^2.1.3"
  },
  "devDependencies": {
    "tsd": "^0.31.2",
    "typescript": "^5.4.2"
  }
}
